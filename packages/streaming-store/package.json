{
  "name": "@incremunica/streaming-store",
  "version": "2.0.0",
  "description": "An RDF Store where the match function gives a stream.",
  "lsd:module": true,
  "license": "MIT",
  "homepage": "https://github.com/comunica",
  "repository": {
    "type": "git",
    "url": "https://github.com/maartyman/incremunica.git",
    "directory": "packages/incremental-rdf-streaming-store"
  },
  "bugs": {
    "url": "https://github.com/comunica/issues"
  },
  "keywords": [
    "rdf",
    "rdfjs",
    "store",
    "source",
    "stream",
    "streaming"
  ],
  "sideEffects": false,
  "main": "lib/index.js",
  "typings": "lib/index",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "lib/**/*.js.map"
  ],
  "scripts": {
    "build": "npm run build:ts && npm run build:components",
    "build:ts": "node \"../../node_modules/typescript/bin/tsc\"",
    "build:components": "componentsjs-generator"
  },
  "dependencies": {
    "@rdfjs/types": "*",
    "@types/n3": "^1.10.4",
    "@types/readable-stream": "^4.0.0",
    "n3": "^1.16.3",
    "rdf-string": "^2.0.0",
    "rdf-terms": "^1.9.1",
    "readable-stream": "^4.3.0"
  },
  "devDependencies": {
    "@incremunica/types": "^2.0.0"
  }
}
